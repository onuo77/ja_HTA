package review0331;

public class MethodOverloadingSample {
	//기본자료형 타입 매개변수를 가진 메소드 정의 후 매개변수 값의 덧셈(합) 출력

	//int x, double y
	void ca(int x, double y) {
		System.out.println("int, double 합");
		double result = x + y;
		System.out.println("x + y의 합 : " + result);
	}
	
	//double x, int y
	void ca(double x, int y) {
		System.out.println("double,int 합");
		double result = x + y;
		System.out.println("x + y의 합 : " + result);
	}

	//int x, int y
	void ca(int x, int y) {
		int result = x + y;
		System.out.println("x + y의 합 : " + result);
	}
	
	//int x,y,z
	void ca(int x, int y, int z) {
		int result = x + y + z;
		System.out.println("x + y + z의 합 : " + result);
	}
	//double x, double y
	void ca(double x, double y) {
		double result = x + y;
		System.out.println("x + y의 합 : " + result);
	}
}
