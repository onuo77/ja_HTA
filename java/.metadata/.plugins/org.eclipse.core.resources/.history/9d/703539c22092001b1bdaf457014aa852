package review0331;

public class User {
	
	//번호, 이름, 아이디, 비밀번호, 이메일, 전화번호, 나이, 주소, 결혼여부 멤버변수 선언
	int no;
	String name;
	String id;
	String password;
	String email;
	String tel;
	int age;
	String address;
	boolean isMarried;
	
	//생성자 메소드에 위의 멤버변수 값을 초기화하기
	User(){
		no = 3000;
		name = "홍길동";
		id = "hong";
		password = "xyz1234";
		email = "";
		tel = "010-0000-0000";
		age = 40;
		address = "";
		isMarried = true;
	}
	//생성자 메소드 중복정의
	User(int no, String name, String id, String password){
		System.out.println("번호 : " + no);
		System.out.println("이름 : " + name);
		System.out.println("아이디 : " + id);
		System.out.println("비밀번호 : " + password);
	}
	//정보값출력 메소드
}
